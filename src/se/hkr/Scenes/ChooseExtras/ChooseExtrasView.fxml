<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="se.hkr.Scenes.ChooseExtras.ChooseExtrasController">
   <children>
      <TableView fx:id="tblOptions" layoutX="15.0" layoutY="61.0" prefHeight="200.0" prefWidth="318.0">
        <columns>
            <TableColumn fx:id="colCar" prefWidth="75.0" text="Car" />
          <TableColumn fx:id="colPrice" prefWidth="75.0" text="Price" />
          <TableColumn fx:id="colName" prefWidth="150.0" text="Name" />
        </columns>
      </TableView>
      <Label layoutX="198.0" layoutY="10.0" prefHeight="51.0" prefWidth="271.0" text="ChooseExtra">
         <font>
            <Font size="28.0" />
         </font></Label>
      <Button layoutX="268.0" layoutY="267.0" mnemonicParsing="false" onAction="#buttonChoosePressed" text="Choose" />
      <TableView fx:id="tblBookedOptions" layoutX="366.0" layoutY="61.0" prefHeight="200.0" prefWidth="209.0">
         <columns>
            <TableColumn fx:id="colBookingCar" prefWidth="75.0" text="Car" />
            <TableColumn fx:id="colBookingName" prefWidth="133.0" text="Name" />
         </columns>
      </TableView>
      <Button layoutX="507.0" layoutY="267.0" mnemonicParsing="false" onAction="#buttonRemovePressed" text="Remove" />
      <TextField fx:id="txtFldPrice" editable="false" layoutX="402.0" layoutY="305.0" />
      <Label layoutX="323.0" layoutY="310.0" text="Total price:" />
      <Button layoutX="522.0" layoutY="345.0" mnemonicParsing="false" onAction="#buttonProceedPressed" text="Next" />
   </children>
</AnchorPane>
