      <TextField fx:id="txtFldLastName" layoutX="212.0" layoutY="90.0" promptText="Last name" styleClass="drop-shadow" />
      <TextField fx:id="txtFldSsn" layoutX="212.0" layoutY="132.0" promptText="Social security number" styleClass="drop-shadow">
